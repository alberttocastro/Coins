<div class="container">
<%= form_with(model: telephone, local: true) do |form| %>
 
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :number %><br>
    <%= form.text_field :number %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
</div>